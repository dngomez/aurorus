{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "time",
      "required": true
    },
    "comment": {
      "type": "text"
    },
    "people": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "email": {
      "type": "email",
      "required": false
    },
    "phone": {
      "type": "biginteger",
      "required": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "number": {
      "type": "uid",
      "required": true
    },
    "client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::client.client",
      "inversedBy": "books"
    }
  }
}
